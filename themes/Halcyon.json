  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8695B7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#BAE67E"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#FFAE57"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#FFAE57"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#C3A6FF"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#A2AABC"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#C3A6FF"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#C3A6FF"
      }
    },
    {
      "name": "Storage modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#C3A6FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "name": "Inherited class",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#5CCFE6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#5CCFE6"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#FFAE57"
      }
    },
    {
      "name": "Tag name",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#5CCFE6"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function",
        "support.macro"
      ],
      "settings": {
        "foreground": "#5CCFE6"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#FFAE57"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#5CCFE6"
      }
    },
    {
      "name": "Library variable",
      "scope": [
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#A2AABC"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#F95972",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#F95972",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#5CCFE6"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FFAE57"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "foreground": "#5CCFE6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#FFAE57",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#C3A6FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#5CCFE6"
      }
    },
    {
      "name": "Markdown - Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#BAE67E"
      }
    }
  ]
}
